<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Transection_One_Record">


    <TextView
        android:id="@+id/txtx"
        android:layout_width="96dp"
        android:layout_height="30dp"
        android:layout_marginStart="56dp"
        android:layout_marginLeft="56dp"
        android:layout_marginTop="112dp"
        android:text="Reciever Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtx1"
        android:layout_width="98dp"
        android:layout_height="30dp"
        android:layout_marginTop="32dp"
        android:text="Transection ID"
        app:layout_constraintStart_toStartOf="@+id/txtx"
        app:layout_constraintTop_toBottomOf="@+id/txtx" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="96dp"
        android:layout_height="28dp"
        android:layout_marginTop="32dp"
        android:text="Amount"
        app:layout_constraintStart_toStartOf="@+id/txtx1"
        app:layout_constraintTop_toBottomOf="@+id/txtx1" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="81dp"
        android:layout_height="28dp"
        android:layout_marginTop="32dp"
        android:text="Scholarship"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/First_Name"
        android:layout_width="146dp"
        android:layout_height="29dp"
        android:layout_marginStart="28dp"
        android:layout_marginLeft="28dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/txtx"
        app:layout_constraintTop_toTopOf="@+id/txtx" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="78dp"
        android:layout_height="28dp"
        android:layout_marginTop="36dp"
        android:text="Status"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/Sure_Name"
        android:layout_width="143dp"
        android:layout_height="29dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/First_Name"
        app:layout_constraintTop_toTopOf="@+id/txtx1" />

    <TextView
        android:id="@+id/AgE"
        android:layout_width="142dp"
        android:layout_height="29dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/Sure_Name"
        app:layout_constraintTop_toTopOf="@+id/textView7" />

    <TextView
        android:id="@+id/EveNt"
        android:layout_width="142dp"
        android:layout_height="29dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/AgE"
        app:layout_constraintTop_toTopOf="@+id/textView8" />

    <TextView
        android:id="@+id/CniC"
        android:layout_width="197dp"
        android:layout_height="32dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/EveNt"
        app:layout_constraintTop_toTopOf="@+id/textView13" />

</androidx.constraintlayout.widget.ConstraintLayout>